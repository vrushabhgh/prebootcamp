const readline = require('readline');
const inp = readline.createInterface({input: process.stdin});
const userInput = [];
inp.on("line", (data) =>
{userInput.push(data);
    
});
inp.on("close", () => {//
var num= userInput[0];
var b = Math.round(num);
if(b !==0)
{
   if(b % 2===0)
{console.log("Even");

}
else
{
console.log("Odd");
}
}
else
{console.log("Zero");
}


});
