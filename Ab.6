const readline = require('readline');
const inp = readline.createInterface({input: process.stdin});
const userInput = [];
inp.on("line", (data) =>
{userInput.push(data);
    
});
inp.on("close", () => {
 
   var a = userInput[0];
   var b=a*1000;
   var c=a*100000;
   console.log(b);
   console.log(c);

});
